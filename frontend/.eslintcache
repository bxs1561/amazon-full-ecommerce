essageBox from \"../components/MessageBox\";\nimport {detailsProduct} from \"../actions/productActions\";\nfunction ProductScreen(props) {\n    const dispatch = useDispatch();\n    const productId = props.match.params.id;\n    // const product = data.products.find(x=>x._id === props.match.params.id)\n    const productDetails = useSelector(state=>state.productDetails);\n    const {loading, error, product} = productDetails;\n    const[qty, setQty] = useState(1)\n\n    useEffect(()=>{\n        dispatch(detailsProduct(productId))\n    },[dispatch])\n\n    const addToCartHandler =()=>{\n        props.history.push(`/cart/${productId}?qty=${qty}`)\n    }\n    return(\n        <div>\n            {loading?(<LoadingBox></LoadingBox>\n            ) : error?(<MessageBox variant=\"danger\">{error}</MessageBox>\n            ):(\n                <div>\n                    <Link to=\"/\">Back to result</Link>\n                    <div className=\"row top\">\n                        <div className=\"col-2\">\n                            <img className=\"large\" src={product.image} alt={product.name}/>\n                        </div>\n                        <div className=\"col-1\">\n                            <ul>\n                                <li>\n                                    <h1>{product.name}</h1>\n                                </li>\n                                <li>\n                                    <Rating\n                                        rating={product.rating}\n                                        numReviews={product.numReviews}\n                                    ></Rating>\n                                </li>\n                                <li>\n                                    Price : ${product.price}\n                                </li>\n                                <li>\n                                    Description:\n                                    <p>{product.description}</p>\n                                </li>\n                            </ul>\n\n                        </div>\n                        <div className=\"col-1\">\n                            <div className=\"card card-body\">\n                                <ul>\n                                    <li>\n                                        <div className=\"row\">\n                                            <div>Price</div>\n                                            <div className=\"price\">\n                                                ${product.price}\n                                            </div>\n                                        </div>\n                                    </li>\n                                    <li>\n                                        <div className=\"row\">\n                                            <div>Status</div>\n                                            <div >\n                                                {product.countInStock>0? (<span className=\"success\"> In Stock </span>\n                                                ):(\n                                                    <span className=\"error\">Unavailable</span>)}\n                                            </div>\n                                        </div>\n                                    </li>\n                                    {\n                                        product.countInStock>0 &&(\n                                            <>\n                                                <li>\n                                                    <div className=\"row\">\n                                                        <div>qty</div>\n                                                        <div>\n                                                            <select value={qty} onChange={event => setQty(event.target.value)}>\n                                                                {/*if countIn stock is 6, below lines return number from 0 to 5*/}\n                                                                {\n                                                                    [...Array(product.countInStock).keys()].map(x=>(\n                                                                        <option key={x+1} value={x+1}>\n                                                                            {x+1}\n                                                                        </option>\n                                                                    ))\n                                                                }\n                                                            </select>\n                                                        </div>\n                                                    </div>\n                                                </li>\n                                            <li>\n                                                <button onClick={addToCartHandler} className=\"primary block\">\n                                                    Add to Cart\n                                                </button>\n                                            </li>\n                                            </>\n                                        )\n                                    }\n                                </ul>\n\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            )\n            }\n        </div>\n    )\n\n}\nexport default ProductScreen\n","/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/store.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/data.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/reducers/productReducers.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/actions/productActions.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/constants/productConstants.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/s