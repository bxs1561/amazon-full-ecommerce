lt={product.name}/>\n                        </div>\n                        <div className=\"col-1\">\n                            <ul>\n                                <li>\n                                    <h1>{product.name}</h1>\n                                </li>\n                                <li>\n                                    <Rating\n                                        rating={product.rating}\n                                        numReviews={product.numReviews}\n                                    ></Rating>\n                                </li>\n                                <li>\n                                    Price : ${product.price}\n                                </li>\n                                <li>\n                                    Description:\n                                    <p>{product.description}</p>\n                                </li>\n                            </ul>\n\n                        </div>\n                        <div className=\"col-1\">\n                            <div className=\"card card-body\">\n                                <ul>\n                                    <li>\n                                        <div className=\"row\">\n                                            <div>Price</div>\n                                            <div className=\"price\">\n                                                ${product.price}\n                                            </div>\n                                        </div>\n                                    </li>\n                                    <li>\n                                        <div className=\"row\">\n                                            <div>Status</div>\n                                            <div >\n                                                {product.countInStock>0? (<span className=\"success\"> In Stock </span>\n                                                ):(\n                                                    <span className=\"error\">Unavailable</span>)}\n                                            </div>\n                                        </div>\n                                    </li>\n                                    {\n                                        product.countInStock>0 &&(\n                                            <>\n                                                <li>\n                                                    <div className=\"row\">\n                                                        <div>qty</div>\n                                                        <div>\n                                                            <select value={qty} onChange={event => setQty(event.target.value)}>\n                                                                {/*if countIn stock is 6, below lines return number from 0 to 5*/}\n                                                                {\n                                                                    [...Array(product.countInStock).keys()].map(x=>(\n                                                                        <option key={x+1} value={x+1}>\n                                                                            {x+1}\n                                                                        </option>\n                                                                    ))\n                                                                }\n                                                            </select>\n                                                        </div>\n                                                    </div>\n                                                </li>\n                                            <li>\n                                                <button onClick={addToCartHandler} className=\"primary block\">\n                                                    Add to Cart\n                                                </button>\n                                            </li>\n                                            </>\n                                        )\n                                    }\n                                </ul>\n\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            )\n            }\n        </div>\n    )\n\n}\nexport default ProductScreen\n","/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/constants/cartConstants.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/components/Rating.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/constants/productConstants.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/actions/cartActions.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/actions/productActions.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/components/MessageBox.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/components/LoadingBox.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/components/Product.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/reportWebVitals.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/reducers/userReducers.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/constants/userConstants.js",[],"/Users/bikramsubedi/Desktop/Amazon mern/frontend/src/actions/userActions.js",[],{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":18,"column":7,"nodeType":"97","endLine":18,"endColumn":17,"suggestions":"98"},"no-native-reassign",["99"],"no-negated-in-lhs",["100"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'productId'. Either include it or remove the dependency array.","ArrayExpression",["101"],"no-global-assign","no-unsafe-negation",{"desc":"102","fix":"103"},"Update the dependencies array to be: [dispatch, pro